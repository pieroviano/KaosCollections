<Project>
	<Import Project="Directory.Nuget.Props" Condition="'$(DirectoryNuGet)'==''" />
	<PropertyGroup>
		<VersionPrefix>$(KaosCollectionsVersion)</VersionPrefix>
		<VersionSuffix Condition="'$(Configuration)'!='Release'">0-at20251222224349</VersionSuffix>
		<VersionSuffix Condition="'$(Configuration)'=='Release'">0</VersionSuffix>
		<SolutionDir Condition="'$(SolutionDir)'==''">$(MSBuildThisFileDirectory)</SolutionDir>
		<LangVersion>latest</LangVersion>
		<DefineConststants>$(DefineConsts);PUBLIC</DefineConststants>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Net4x.NuGetUtility" Version="$(NuGetUtilityVersion)" PrivateAssets="All" />
	</ItemGroup>
</Project>